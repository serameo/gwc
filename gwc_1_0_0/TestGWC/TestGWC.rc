    MENUITEM "New &Hello", IDM_FILE_NEW
    MENUITEM "New &Rectangle", IDM_FILE_NEW_RECT
    MENUITEM "&Close", IDM_WINDOW_CLOSE
    MENUITEM SEPARATOR
    MENUITEM "E&xit\tAlt + F4", IDM_FILE_EXIT
  END
  POPUP "&Window"
  BEGIN
    MENUITEM "&Tile", IDM_WINDOW_TILE
    MENUITEM "&Arrange", IDM_WINDOW_ARRANGEICONS
    MENUITEM "&Close", IDM_WINDOW_CLOSEALL
  END
